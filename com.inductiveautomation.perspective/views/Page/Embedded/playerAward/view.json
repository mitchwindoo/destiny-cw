          }
        ],
        "type": "expr-struct"
      },
      "persistent": true
    },
    "custom.playerInfo": {
      "binding": {
        "config": {
          "parameters": {
            "destinyid": "{view.custom.awardDetails.playerId}"
          },
          "queryPath": "destiny/clans/getPlayerInfo",
          "returnFormat": "json"
        },
        "transforms": [
          {
            "code": "\treturn value[0]",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
